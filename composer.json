{
    "name": "jeanlrnt/laravel-cancellable",
    "description": "An cancellable trait package for Laravel Eloquent models",
    "keywords": ["laravel", "laravel-cancellable"],
    "license": "MIT",
    "homepage": "https://github.com/jeanlrnt/laravel-cancellable",
    "type": "library",
    "support": {
        "issues": "https://github.com/jeanlrnt/laravel-cancellable/issues",
        "source": "https://github.com/jeanlrnt/laravel-cancellable"
    },
    "authors": [
        {
            "name": "Jean Laurent",
            "email": "jeanz.laurent@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "illuminate/pagination": "^7.0|^8.0|^9.0|^10.0",
        "orchestra/testbench": "^6.3|^7.0|^8.0",
        "phpunit/phpunit": "^8.4|^9.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelCancellable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelCancellable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "LaravelCancellable\\LaravelCancellableServiceProvider"
            ]
        }
    }
}
